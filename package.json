{
  "name": "@cosmosreverse/cosmos",
  "version": "0.0.1",
  "description": "Use stencil to build react component.",
  "license": "MIT",
  "author": "tomoe",
  "keywords": [
    "web-components",
    "reactive",
    "vue",
    "react"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/timo/timo.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --prod --docs",
    "build:website": "cd ./website/ && rm -rf node_modules && npm i && npm run build",
    "serve": "stencil build --dev --watch --serve",
    "generate": "stencil generate",
    "deploy": "git submodule update && npm run build:website && gh-pages -d ./website/src/.vuepress/dist",
    "prettier": "npx prettier --write ./src"
  },
  "devDependencies": {
    "@stencil/core": "2.6.0",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/store": "^1.3.0",
    "@stencil/svelte-output-target": "0.0.3",
    "@stencil/vue-output-target": "^0.2.5",
    "@types/events": "^3.0.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "gh-pages": "^2.2.0",
    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "prettier": "2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "svelte": "^3.31.2",
    "typescript": "^3.9.7",
    "vue": "^3.0.5"
  }
}
